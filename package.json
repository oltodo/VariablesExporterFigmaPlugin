{
  "name": "variables-exporter",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.11.0",
  "description": "Export Figma variables to JSON-based format",
  "author": "",
  "license": "",
  "main": "main.js",
  "scripts": {
    "build": "pnpm run build-main && pnpm run build-ui",
    "build-main": "tsc -p tsconfig.plugin.json",
    "build-ui": "vite build",
    "watch": "concurrently \"pnpm run build-main --watch\" \"pnpm run build-ui --watch\"",
    "lint": "eslint",
    "lint-fix": "eslint --fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "radash": "^12.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.3.0",
    "use-debounce": "^10.0.4",
    "valibot": "^1.1.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.1",
    "@eslint-react/eslint-plugin": "^1.49.0",
    "@eslint/eslintrc": "^3.3.1",
    "@figma/plugin-typings": "*",
    "@tailwindcss/vite": "^4.1.7",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "lint-staged": "^16.0.0",
    "simple-git-hooks": "^2.13.0",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-singlefile": "^2.2.0",
    "vitest": "^3.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
